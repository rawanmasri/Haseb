        <activity
            android:name=".DisplayLoginActivity"
            android:label="@string/title_activity_display_login" >
        </activity>
        <activity
            android:name=".HomePage"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/title_activity_home_page" >
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home" >
        </activity>
        <activity
            android:name=".MyAccountActivity"
            android:label="@string/title_activity_my_account" >
        </activity>
        <activity
            android:name=".NotificationActivity"
            android:label="@string/title_activity_notification" >
        </activity>
        <activity
            android:name=".DoneSignup"
            android:label="@string/title_activity_done_signup" >
        </activity>
        <activity
            android:name=".EMainActivity"
            android:label="@string/title_activity_emain" >
